{
  "name": "@vernisai/api",
  "version": "0.1.0",
  "description": "API for VernisAI - AI-powered chat and intelligent agents",
  "author": "Monsoft Solutions - https://monsoftsolutions.com",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "lint": "eslint src/",
    "dev": "ts-node --transpile-only src/server.ts",
    "dev:server": "cross-env API_SERVER_MODE=server node dist/server.js",
    "start:server": "cross-env API_SERVER_MODE=server node dist/server.js",
    "generate:openapi": "tsx src/scripts/generate-openapi.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@monsoft/vernisai-config": "*",
    "@trpc/server": "^10.44.1",
    "@vernisai/database": "*",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "next": "^14.0.4",
    "superjson": "^2.2.1",
    "trpc-openapi": "^1.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.136",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/node": "^20.17.25",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vernisai/testing": "*",
    "@vitest/coverage-v8": "^3.0.9",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.1",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typescript": "^5.3.3",
    "vitest": "^3.0.9"
  }
}
